<h1>新規掲示板作成</h1>
<h2>new.html.erb</h2>

<%= form_with(model: @board, url: boards_path) do |f|%>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :img %>
  <%= f.text_field :img %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit "新規投稿" %>

<% end %>

